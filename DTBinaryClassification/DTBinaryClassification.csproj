<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.ML" Version="1.7.1" />
    <PackageReference Include="Microsoft.ML.FastTree" Version="1.7.1" />
    <PackageReference Include="Microsoft.ML.LightGbm" Version="1.7.1" />
  </ItemGroup>
  <ItemGroup Label="DTBinary">
    <None Include="DTBinary.consumption.cs">
      <DependentUpon>DTBinary.mbconfig</DependentUpon>
    </None>
    <None Include="DTBinary.training.cs">
      <DependentUpon>DTBinary.mbconfig</DependentUpon>
    </None>
    <None Include="DTBinary.zip">
      <DependentUpon>DTBinary.mbconfig</DependentUpon>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>